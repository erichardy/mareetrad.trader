<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">

<object name="object_buttons" meta_type="CMF Action Category">
  <property name="title"></property>

  <object name="create_dataset" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">create dataset</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/@@createdataset</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Plone Site Setup: Overview"/>
   </property>
   <property name="visible">True</property>
  </object>

  <object name="purge_dataset" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">purge dataset</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/@@createdataset?purge=1</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Plone Site Setup: Overview"/>
   </property>
   <property name="visible">True</property>
  </object>

  <object name="import-csv" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Import CSV Traders</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object_url}/@@importCSV-traders</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="Plone Site Setup: Overview"/>
   </property>
   <property name="visible">True</property>
  </object>


</object>

</object>